# CLI Panda Environment Variables Example
# Copy this file to .env and fill in your values

# LM Studio Configuration
LMSTUDIO_HOST=http://localhost:1234
LMSTUDIO_API_KEY=lm-studio

# Default Model
DEFAULT_MODEL=qwen3-8b

# Logging
LOG_LEVEL=info
LOG_FILE=cli-panda.log

# Python Environment
PYTHONPATH=./

# Development
DEBUG=false
DEV_MODE=false